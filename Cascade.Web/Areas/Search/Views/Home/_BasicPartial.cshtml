@model IList<Cascade.Data.Models.SearchResult>
<link href="@Url.Content("~/Content/dataTables/demo_table.css")" rel="stylesheet" type="text/css" />

<script src="~/Scripts/datatable/jquery-1.4.4.min.js" type="text/javascript"></script>
<script src="~/Scripts/datatable/index.js" type="text/javascript"></script>
<script src="~/Scripts/datatable/jquery.dataTables.min.js" type="text/javascript"></script>
<h2>Search Rsults:</h2>
<div id="divsearchRecords">
    <table id="searchRecords" class="display">
        <thead>
            <tr>
                <th></th>
                <th>Name</th>
                <th>ProductDescription</th>
                <th>WorkStatusDescription</th>
                <th>RespAgency</th>
                <th>StatusDescription</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var searchRecord in Model)
            {
                <tr>
                    <td>@Html.ActionLink("View", "Details", new { id = searchRecord.ACCOUNT })</td>
                    <td>@searchRecord.Name</td>
                    <td>@searchRecord.ProductDescription</td>
                    <td>@searchRecord.WorkStatusDescription</td>
                    <td>@searchRecord.RespAgency</td>
                    <td>@searchRecord.StatusDescription</td>

                </tr> 
            }
        </tbody>
    </table>
</div>
@section scripts{
}